import java.util.Scanner;

public class a053 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan = new Scanner(System.in);
		while (scan.hasNext()) {
			int num = scan.nextInt();
			int sum = 0;
			if (num <= 10) {
				sum = 6 * num;
			} else {
				if (num <= 20) {
					sum = 60 + (num - 10) * 2;
				} else {
					if (num <= 40) {
						sum = 80 + (num - 20) * 1;
					} else {
						sum = 100;
					}
				}
			}
			System.out.println(sum);
		}
	}

}
