import java.util.Scanner;

public class b761 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan=new Scanner(System.in);
		
		while(scan.hasNext()){
			int w=scan.nextInt();
			if(w%4==0){
				System.out.printf("egg");
			}
			else{
				System.out.printf("fat");
			}
			
			
		}
	}

}
