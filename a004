import java.util.Scanner;

public class a004 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan=new Scanner(System.in);
		
		while(scan.hasNext()){
			
			
			
			int a=scan.nextInt();
			if(a%400==0){
				System.out.println("閏年");
			}else{
				if(((a%4)==0)&&((a%100)!=0)){
					System.out.println("閏年");
				}
				else{
					System.out.println("平年");
				}
			}
			
		}
	}

}
