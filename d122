import java.util.Scanner;

public class d122 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan = new Scanner(System.in);
		while (scan.hasNext()) {
			int n = scan.nextInt();
			int sum = 0;
			int a = 1;
			while (Math.pow(5, a) <= n) {
				sum += (int) n / Math.pow(5, a);
				a++;

			}
			System.out.println(sum);
		}
	}

}
