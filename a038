import java.util.Scanner;

public class a038 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan=new Scanner(System.in);
		
		while(scan.hasNext()){
			int a=scan.nextInt();
			if(a!=0){
				while(a%10==0){
					a=a/10;
				}
				while(a>10){
					System.out.print(a%10);
					a=(int)(a/10);
				}
				System.out.println(a);
			}else{
				System.out.println("0");
			}
		
			
		}
		
	}

}
