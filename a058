import java.util.Scanner;

public class a058 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan=new Scanner(System.in);
		while(scan.hasNext()){
		int num=scan.nextInt();
		int m0=0,m1=0,m2=0;
		for(int i=0;i<num;i++){
			int in=scan.nextInt();
			switch(in%3){
			case 0:
				m0++;
				break;
			case 1:
				m1++;
				break;
			default:
				m2++;
			break;
			}
		
		}
		System.out.printf("%d %d %d%n",m0,m1,m2);
	}
	}
}
